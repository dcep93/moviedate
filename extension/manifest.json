{
	"name": "Uh Oh Synchy",
	"description": "Synchs videos for people watching the same video at the same time.",
	"version": "1.0.0",
	"icons": {
		"16": "uhohsynchy.png",
		"48": "uhohsynchy.png",
		"128": "uhohsynchy.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.dropbox.com/*",
				"https://www.youtube.com/*"
			],
			"js": ["content_script.js"]
		}
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "uhohsynchy.png"
	},
	"permissions": ["identity", "identity.email"],
	"manifest_version": 2,
	"content_security_policy": "script-src 'self' https://*.firebaseio.com; object-src 'self'"
}
