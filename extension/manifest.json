{
	"name": "Uh Oh Synchy",
	"description": "Synchs videos for people watching the same video at the same time.",
	"version": "0.0.1",
	"permissions": [],
	"icons": {
		"16": "uhohsynchy.png",
		"48": "uhohsynchy.png",
		"128": "uhohsynchy.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.dropbox.com/*",
				"https://www.youtube.com/*"
			],
			"js": ["content_script.js"]
		}
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "uhohsynchy.png"
	},
	"manifest_version": 2
}
